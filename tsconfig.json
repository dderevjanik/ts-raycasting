{
    "compilerOptions": {
        "declaration": true,
        "module": "commonjs",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitUseStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noLib": false,
        "outDir": "dist",
        "preserveConstEnums": true,
        "removeComments": false,
        "sourceMap": false,
        "strictNullChecks": true,
        "target": "es5"
    },
    "files": [
        "src/RayCast.ts",
        "src/LookupTables.ts"
    ]
}
