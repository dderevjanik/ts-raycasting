{
  "name": "ts-raycasting",
  "version": "0.0.8",
  "description": "Raycasting in typescript",
  "main": "dist/RayCast.js",
  "typescript": {
    "definition": "dist/RayCast.d.ts"
  },
  "scripts": {
    "build": "npm run build:web && npm run build:node && npm run build:example",
    "build:web": "webpack --config webpack.config.web.js",
    "build:node": "tsc",
    "build:example": "tsc --project tsconfig.example.json",
    "dev": "webpack-dev-server --config webpack.config.web.js",
    "lint": "tslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dderevjanik/ts-raycasting"
  },
  "keywords": [
    "rays",
    "raycast",
    "raycasting",
    "2.5d",
    "pseudo2d"
  ],
  "author": {
    "name": "Daniel Derevjanik",
    "email": "daniel.derevjanik@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "ts-loader": "^0.8.1",
    "tslint": "^3.5.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
