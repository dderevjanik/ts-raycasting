{
  "name": "ts-raycasting",
  "version": "0.0.12",
  "description": "Raycasting in typescript",
  "main": "dist/RayCast.js",
  "typescript": {
    "definition": "dist/RayCast.d.ts"
  },
  "typings": "dist/RayCast.d.ts",
  "scripts": {
    "build": "npm run build:web && npm run build:node && npm run build:examples",
    "build:web": "webpack --config webpack.config.web.js",
    "build:node": "tsc",
    "build:examples": "tsc --project examples/",
    "test": "mocha test/**/*.test.js",
    "test-dev": "mocha test/**/*.test.js --watch",
    "dev": "webpack-dev-server --config webpack.config.web.js",
    "lint": "node_modules/.bin/tslint src/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dderevjanik/ts-raycasting"
  },
  "keywords": [
    "rays",
    "raycast",
    "raycasting",
    "2.5d",
    "pseudo3d"
  ],
  "author": {
    "name": "Daniel Derevjanik",
    "email": "daniel.derevjanik@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "mocha": "^3.2.0",
    "ts-loader": "^1.3.3",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
